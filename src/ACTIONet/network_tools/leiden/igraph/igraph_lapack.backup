                            WORK, IWORK, INFO )   

         INTEGER            DOL, DOU, INFO, LDZ, M, N   
         DOUBLE PRECISION   MINRGP, PIVMIN, RTOL1, RTOL2, VL, VU   
         INTEGER            IBLOCK( * ), INDEXW( * ), ISPLIT( * ),   
        $                   ISUPPZ( * ), IWORK( * )   
         DOUBLE PRECISION   D( * ), GERS( * ), L( * ), W( * ), WERR( * ),   
        $                   WGAP( * ), WORK( * )   
